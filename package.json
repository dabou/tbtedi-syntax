{
    "name": "tbtedi-syntax",
    "displayName": "tbtedi-syntax",
    "description": "Support TbtEdi Studio file",
    "version": "0.1.3",
    "publisher": "tbtedi-syntax",
    "engines": {
        "vscode": "^1.17.0"
    },
    "license": "SEE LICENSE IN LICENSE.txt",
    "categories": [
        "Languages",
        "Snippets"
    ],
    "homepage": "https://github.com/dabou/tbtedi-syntax",
    "bugs": {
        "url": "https://github.com/dabou/tbtedi-syntax/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/dabou/tbtedi-syntax.git"
    },
    "galleryBanner": {
        "color": "#A4D9E3"
    },
    "contributes": {
        "languages": [
            {
                "id": "tbtedi",
                "aliases": [
                    "Tbtedi",
                    "tbtedi"
                ],
                "extensions": [
                    ".cedsc",
                    ".ceguide",
                    ".celink",
                    ".swptable"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "tbtedi",
                "scopeName": "source.celink",
                "path": "./syntaxes/tbtedi.tmLanguage.json"
            }
        ],
        "snippets": [
            {
                "language": "tbtedi",
                "path": "./snippets/tbtedi.json"
            }
        ]
    }
}